    <div id="testimonial">
        <div class="testimonial-content">
            <div class="testimonial-grid">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="testimonial-title text-left wow fadeIn">
                                <h3>NEWS</h3>
                                <div class="underline1 white"></div>
                                <div class="underline2 white bottom"></div>
                            </div><!-- end testimonial-title  -->
                        </div>
                    </div>
                </div>
                <div class="testimonial-details wow fadeIn text-left">
                    <section id="carousel">
                        <div class="container">
                            <div class="row">
                                <div class="col-md-8">
                                    <div class="carousel slide" id="fade-quote-carousel" data-ride="carousel" data-interval="3000">
                                        <ol class="carousel-indicators carousel-indicators1">
                                            @foreach (var item in Model.FirstOrDefault().news)
                                            {
                                                if (j == 0)
                                                {
                                                    <li data-target="#fade-quote-carousel" data-slide-to="@j" class="active"></li>
                                                }
                                                else
                                                {
                                                    <li data-target="#fade-quote-carousel" data-slide-to="@j"></li>
                                                }
                                                j++;
                                            }
                                        </ol>
                                        <div class="carousel-inner">
                                            @foreach (var item in Model.FirstOrDefault().news)
                                            {
                                                if (i == 0)
                                                {
                                                    <div class="active item">
                                                        <blockquote>
                                                            @Html.Raw(item.Description)
                                                            <br />
                                                            <p>@item.Title</p>
                                                            <div class="star-icon-grid text-left">
                                                                <i class="fa fa-star-o special-item-icon" aria-hidden="true"></i>
                                                                <i class="fa fa-star-o special-item-icon" aria-hidden="true"></i>
                                                                <i class="fa fa-star-o special-item-icon" aria-hidden="true"></i>
                                                                <i class="fa fa-star-o special-item-icon" aria-hidden="true"></i>
                                                                <i class="fa fa-star-o special-item-icon" aria-hidden="true"></i>
                                                            </div><!-- end star-icon-grid -->
                                                        </blockquote>
                                                    </div><!-- end item  -->
                                                }
                                                else
                                                {
                                                    <div class="item">
                                                        <blockquote>
                                                            @Html.Raw(item.Description)
                                                            <br />
                                                            <p>@item.Title</p>
                                                            <div class="star-icon-grid text-left">
                                                                <i class="fa fa-star-o special-item-icon" aria-hidden="true"></i>
                                                                <i class="fa fa-star-o special-item-icon" aria-hidden="true"></i>
                                                                <i class="fa fa-star-o special-item-icon" aria-hidden="true"></i>
                                                                <i class="fa fa-star-o special-item-icon" aria-hidden="true"></i>
                                                                <i class="fa fa-star-o special-item-icon" aria-hidden="true"></i>
                                                            </div><!-- end star-icon-grid -->
                                                        </blockquote>
                                                    </div><!-- end item  -->
                                                }
                                                i++;
                                            }


                                        </div><!-- end carousel-inner  -->
                                    </div><!-- end carousel-slide  -->
                                </div><!-- end col-md-8  -->
                            </div><!-- end row  -->
                        </div><!-- end container  -->
                    </section>
                </div><!-- end testimonials-details  -->
            </div><!-- end testimonial-grid  -->
        </div><!-- end testimonial-content  -->
    </div><!-- end testimonial  -->
