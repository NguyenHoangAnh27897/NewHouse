    <div id="contact">
        <div id="contact-parallex">
            <div class="contact-grid">
                <div class="contact-form-details">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="contact-title text-left wow fadeIn">
                                    <!-- <h3>liên hệ, giải pháp, đối tác, ngân hàng tác phẩm, tin tức</h3> -->
                                    <h3>CONTACT</h3>
                                    <div class="underline1"></div>
                                    <div class="underline2"></div>
                                    <p>New House 2018</p>
                                </div><!-- end contact-title -->
                            </div><!-- end col-md-12  -->
                        </div><!-- end row  -->
                        <div class="row about-row">
                            <div class="col-md-8 col-sm-8 wow fadeIn">
                                <div class="contact-form">
                                    <form class="contact-form shake" action="/home/contact" method="post">
                                        <div class="form-group">
                                            <div class="controls">
                                                <input type="text" id="name" name="name" class="form-control" placeholder="Name" required data-error="Please enter your name">
                                                @*<div class="help-block with-errors"></div>*@
                                            </div><!-- end controls -->
                                        </div><!-- end form-group -->
                                        <div class="form-group">
                                            <div class="controls">
                                                <input type="email" class="email form-control" id="email" name="email" placeholder="Email" required data-error="Please enter your email">
                                                @*<div class="help-block with-errors"></div>*@
                                            </div><!-- end controls -->
                                        </div><!-- end form-group -->
                                        <div class="form-group">
                                            <div class="controls">
                                                <input type="text" id="msg_subject" class="form-control" name="subject" placeholder="Subject" required data-error="Please enter your message subject">
                                                @*<div class="help-block with-errors"></div>*@
                                            </div><!-- end controls -->
                                        </div><!-- end form-group -->
                                        <div class="form-group">
                                            <div class="controls">
                                                <textarea id="message" rows="7" placeholder="Massage" class="form-control" required data-error="Write your message" name="mess"></textarea>
                                                @*<div class="help-block with-errors"></div>*@
                                            </div><!-- end controls -->
                                        </div><!-- end form-group -->
                                        <button type="submit" id="submit" class="btn btn-success"> Send</button>
                                        @*<div id="msgSubmit" class="h3 text-center hidden"></div>*@
                                        <div class="clearfix"></div>
                                    </form>
                                </div><!-- end contact-form -->
                            </div><!-- end col-md-8 -->
                            <div class="col-md-4 col-sm-4">
                                <div class="contact-col" data-wow-delay=".2s">
                                    <i class="fa fa-map-marker contact-icon" aria-hidden="true"></i>
                                    <h3>Địa chỉ : @Model.FirstOrDefault().ab.Address</h3>
                                </div><!-- end contact-col  -->
                                <div class="contact-col" data-wow-delay=".4s">
                                    <i class="fa fa-envelope contact-icon" aria-hidden="true"></i>
                                    <h3>Email : @Model.FirstOrDefault().ab.Email</h3>
                                </div><!-- end contact-col  -->
                                <div class="contact-col" data-wow-delay=".6s">
                                    <i class="fa fa-mobile contact-icon" aria-hidden="true"></i>
                                    <h3>Điện thoại : @Model.FirstOrDefault().ab.Phone</h3>
                                </div><!-- end contact-col  -->
                            </div><!-- end col-md-4 -->
                        </div><!-- end row -->
                    </div><!-- end container -->

                </div><!-- end contact-form-details -->
