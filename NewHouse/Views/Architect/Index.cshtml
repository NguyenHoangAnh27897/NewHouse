@model IEnumerable<NewHouse.Models.Architect>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="home">
    <div id="home-parallex-architect1" class="section" style="background: url(../../Images/NewHouse/imagearchitec/@Model.FirstOrDefault().architect.Avatar) 50% 0 no-repeat fixed;">
        <div class="home-content-main">
            <div class="table-cell">
                <div class="container visible-container">
                    <div class="row home-row">
                        <div class="col-md-12 col-sm-12 col-xs-12">
                            <div class="home-text-architect text-left">
                                <h3>@Model.FirstOrDefault().architect.Name</h3>
                            </div><!-- end home-text  -->
                        </div><!-- end col-md-10 -->
                    </div><!-- end row  -->
                </div><!-- end container -->
            </div><!-- end table-cell -->
        </div><!-- end home-content-main -->
        <div class="parallax" data-velocity="-.1"></div>
        <div class="parallax" data-velocity="-.5" data-fit="525"></div>
    </div><!-- end home-parallex  -->
</div><!-- end home  -->
<!-- ============================================== TEAM ================= -->
<div id="project">
    <div class="team-content1">
        <div class="team-grid text-left">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="team-title text-left wow fadeIn">
                            <h3>Dự án</h3>
                            <div class="underline1"></div>
                            <div class="underline2"></div>
                        </div><!-- end team-title  -->
                    </div><!-- end col-md-12  -->
                </div><!-- end row  -->

                <div class="row team-row text-left">
                    @foreach (var item in Model.FirstOrDefault().project)
                    {
                        <div class="col-md-3 col-sm-6 team-col">
                            <div class="corporate-team-project">
                                <div class="team-member-project wow fadeIn" data-wow-delay=".2s">
                                    <div class="overlay text-left">
                                        <div class="overlay-main">
                                            <div class="table-cell">
                                                <a href="/project/detail/@item.ID">
                                                    <h3>@item.ProjectName</h3>
                                                    <p></p>
                                                </a>
                                                <div class="team-icon-grid">
                                                    <div class="team-icon text-left">
                                                        <ul class="team-social">
                                                            <li><a href="www.facebook.com" target="newtab"><i class="fa fa-facebook" aria-hidden="true"></i></a></li>
                                                            <li><a href="www.twitter.com" target="newtab"><i class="fa fa-twitter" aria-hidden="true"></i></a></li>
                                                            <li><a href="www.pinterest.com" target="newtab"><i class="fa fa-pinterest-p" aria-hidden="true"></i></a></li>
                                                            <li><a href="www.instagram.com" target="newtab"><i class="fa fa-instagram" aria-hidden="true"></i></a></li>
                                                        </ul>
                                                    </div><!-- end team-icon  -->
                                                </div><!-- end team-icon-grid  -->
                                            </div><!-- end table-cell  -->
                                        </div><!-- end overlay-main  -->
                                    </div><!-- end overlay  -->
                                    <img src="~/Images/NewHouse/imageproject/@item.Avatar" alt="" class="team-img-project">
                                </div><!-- end team-member  -->
                                <div class="team-details">
                                    <a href="/project/detail/@item.ID"><h3>@item.ProjectName</h3></a>
                                </div><!-- end team-details  -->
                            </div><!-- end corporate-team  -->
                        </div><!-- end col-md-3  -->
                    }


                </div><!-- end row  -->
            </div><!-- end container  -->
        </div><!-- end row-grid  -->
    </div><!-- end team-content  -->
</div>
<div class="copyright-details">
    <div class="copyright">
        <h3>copyright and design: Newhouse Building Solution</h3>
    </div><!-- end copyright -->
</div><!-- end copyright-details -->
