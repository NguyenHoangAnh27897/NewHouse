    <script type="text/javascript" src="~/Content/NewHouse/js/jquery.fancybox.pack.js?v=2.1.5"></script>
    <script type="text/javascript" src="~/Content/NewHouse/js/fancybox-script.js"></script>
    <script type="text/javascript" src="~/Content/NewHouse/js/fancybox.js"></script>
    <script type="text/javascript" src="~/Content/NewHouse/js/mixitup.js"></script>
    <script src="~/Content/NewHouse/js/jquery.counterup.min.js"></script>
    <script src="~/Content/NewHouse/js/EasePack.min.js"></script>
    <script src="~/Content/NewHouse/js/video.js"></script>
</body><!-- end body -->
</html><!-- end html -->